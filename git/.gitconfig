[user]
	email = m@kuchen.io
	name = Markus Cisler
[core]
	editor = vim
[alias]
	br = branch
	co = checkout
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	rem = remote
	st = status
	up = "!git remote update -p; git merge --ff-only @{u}"
[color]
	ui = true
	branch = auto
	diff = auto
	grep = auto
[diff]
	tool = vimdiff
[push]
	default = simple
[grep]
	extendRegexp = true
	lineNumber = true
[github]
	user = bl1nk
